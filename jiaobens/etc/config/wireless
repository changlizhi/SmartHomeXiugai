
config wifi-device 'radio0'
	option type 'ralink'
	option variant 'mt7628'
	option country 'CN'
	option hwmode '11g'
	option htmode 'HT40'
	option channel 'auto'
	option disabled '0'
	option linkit_mode 'sta'

config wifi-iface 'ap'
	option device 'radio0'
	option mode 'ap'
	option network 'lan'
	option ifname 'ra0'
	option encryption 'none'
	option ssid '迪恩摩因-0011'

config wifi-iface 'sta'
	option device 'radio0'
	option mode 'sta'
	option network 'wan'
	option ifname 'apcli0'
	option led 'mediatek:orange:wifi'
	option encryption 'psk'
	option disabled '0'
	option ssid 'ssssssssssss'
	option key 'adminclz'

